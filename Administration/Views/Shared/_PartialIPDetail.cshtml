@{
    Layout = "";
    var customerIp = ViewData["customerIp"];
}
    <script>
         $.get("https://ipinfo.io/@customerIp?token=e7668de042bf7d", function (response) {
             var element = '<table class="table table-hover table-bordered" style="max-width: 400px;    margin-top: 10px;">' +
                 '<tbody>' +
                 '<tr>' +
                 '<td>' + '城市' + '</td>' +
                 '<td>' + response.city + '</td>' +
                 '</tr>' +
                 '<tr>' +
                 '<td>' + '国家' + '</td>' +
                 '<td>' + response.country + '</td>' +
                 '</tr>' +
                 '<tr>' +
                 '<td>' + 'IP' + '</td>' +
                 '<td>' + response.ip + '</td>' +
                 '</tr>' +
                 '<tr>' +
                 '<td>' + '经纬度' + '</td>' +
                 '<td><a target="google" href="https://www.google.com/maps/@@' + response.loc + ',10z">' + response.loc + '</a></td>' +
                 '</tr>' +
                 '<tr>' +
                 '<td>' + 'org' + '</td>' +
                 '<td>' + response.org + '</td>' +
                 '</tr>' +
                 '<tr>' +
                 '<td>' + '州 省' + '</td>' +
                 '<td>' + response.region + '</td>' +
                 '</tr>' +
                 '</tbody>' +
                 '</table>';
             $("#showDetail").html(element);
         }, "jsonp");
    </script>
<div id="showDetail"></div>

